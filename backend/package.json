{
  "name": "ai-platform-backend",
  "version": "1.0.1",
  "description": "Production-ready backend API for Enterprise AI Hub",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npx prisma generate",
    "postinstall": "npx prisma generate",
    "init-db": "node scripts/init-database.js",
    "seed-db": "node scripts/seed-database.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js .",
    "lint:fix": "node ./node_modules/eslint/bin/eslint.js . --fix",
    "format": "node ./node_modules/prettier/bin/prettier.cjs --write \"**/*.{js,json,md}\"",
    "format:check": "node ./node_modules/prettier/bin/prettier.cjs --check \"**/*.{js,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "load:test": "artillery run artillery.yml",
    "load:quick": "artillery quick --count 10 --num 25 http://localhost:3001/api/health"
  },
  "keywords": [
    "ai",
    "enterprise",
    "api",
    "authentication",
    "postgresql"
  ],
  "author": "SecureMaxTech",
  "license": "ISC",
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/client": "^6.19.0",
    "@sentry/node": "^10.25.0",
    "@sentry/profiling-node": "^10.25.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "googleapis": "^164.1.0",
    "groq-sdk": "^0.36.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "pdf-parse": "^1.1.4",
    "pdfjs-dist": "^5.4.449",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^8.2.0",
    "response-time": "^2.3.2",
    "sanitize-html": "^2.17.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "unpdf": "^1.4.0",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "word-extractor": "^1.0.4"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "artillery": "^2.0.26",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
