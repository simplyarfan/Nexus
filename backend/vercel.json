{
  "version": 2,
  "builds": [
    {
      "src": "api/admin.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/analytics.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cv-intelligence.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/interview-coordinator.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/support.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/tickets.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth"
    },
    {
      "src": "/api/tickets/(.*)",
      "dest": "/api/tickets"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "/api/admin"
    },
    {
      "src": "/api/analytics/(.*)",
      "dest": "/api/analytics"
    },
    {
      "src": "/api/cv-intelligence/(.*)",
      "dest": "/api/cv-intelligence"
    },
    {
      "src": "/api/interview-coordinator/(.*)",
      "dest": "/api/interview-coordinator"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
